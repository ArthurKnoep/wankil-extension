{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Live checker - Wankil Studio",
  "author": "Arthur Knoepflin",
  "manifest_version": 2,
  "version": "1.4.1",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "description": "Soyez le premier inform√© lorsque Laink et Terracid sont en live ;)",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "unlimitedStorage",
    "storage",
    "http://api.twitch.tv/kraken/streams/lainkk"
  ],
  "web_accessible_resources": [
    "img/icon.png"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "display.html",
    "default_title": "Wankil Studio"
  }
}
